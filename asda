#employee_leave_records
php artisan make:migration create_employee_leave_records_table --create=employee_leave_records
#php artisan make:migration alter_employee_leave_records_table --table=employee_leave_records
php artisan make:seeder EmployeeLeaveRecordTableSeeder
php artisan make:model EmployeeLeaveRecord
php artisan make:controller EmployeeLeaveRecordController --resource
#php artisan db:seed --class=EmployeeLeaveRecordTableSeeder

